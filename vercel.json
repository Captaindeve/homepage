{
    "env": {
        "GITHUB_TOKEN": "@github_token"
    },
    "rewrites": [
        {
            "source": "/docs/:match*",
            "destination": "https://wiki.openmultiplayer.now.sh/docs/:match*"
        }
    ],
    "redirects": [
        {
            "source": "/l/:match*",
            "destination": "https://:match*"
        }
    ]
}